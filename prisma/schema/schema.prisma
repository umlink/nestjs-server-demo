generator client {
  provider        = "prisma-client-js"
  output          = "./client"
  previewFeatures = ["prismaSchemaFolder"]
  relationMode    = "prisma"
}

generator docs {
  provider = "node node_modules/prisma-docs-generator"
  output   = "./docs"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}
